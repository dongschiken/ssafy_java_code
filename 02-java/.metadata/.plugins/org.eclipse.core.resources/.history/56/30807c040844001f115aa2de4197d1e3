package com.ssafy.hw.day03.step04;

public class Restaurant {
	
	int resId;
	String name;
	String address;
	String signatureMenu;
	int rate;
	
	public Restaurant(int resId, String name, String address, String signatureMenu, int rate) {
		super();
		this.resId = resId;
		this.name = name;
		this.address = address;
		this.signatureMenu = signatureMenu;
		this.rate = rate;
	}

	public Restaurant() {}
	
	@Override
	public String toString() {
		return String.format("Restaurant [resId=%d, name=%s, address=%s, SignatureMenu=%s, rate=%d"
				,resId
				,name
				,address
				,signatureMenu
				,rate
				+"]");
	}
	
}
