package modifier00_jvm_static;

public class PersonTest {
    public static void main(String[] args) {
    	
    	System.out.println(Person.pCount);
    	Person p1 = new Person();
    	
    	Person.pCount = 100;
    	p1.pCount = 10;
    	System.out.println(Person.pCount);
    	System.out.println(p1.pCount);
    	
    }
}
