package solved;

import java.util.Scanner;

public class EvenSum {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		int ans = 0;
		for(int i = 2; i <= N; i++){
			if(i % 2 == 0){
				ans += i;
				System.out.println(ans);
			}
		}
		System.out.println(ans);
	}
}