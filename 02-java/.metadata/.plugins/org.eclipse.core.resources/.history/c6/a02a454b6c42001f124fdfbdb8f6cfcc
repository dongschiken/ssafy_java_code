package java06_offline;

import java.util.Scanner;

public class GuguTest {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("단을 입력하세요");
		int n = sc.nextInt();
		for (int i = n; i <= n; i++) {
			for (int j = 1; j <= 9; j++) {
				System.out.printf("%d * %d = %d", i, j, i*j);
			}
		}
	}
}
