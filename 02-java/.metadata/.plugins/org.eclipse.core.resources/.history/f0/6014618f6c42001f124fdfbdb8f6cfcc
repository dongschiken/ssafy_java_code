package java06_offline;

import java.util.Scanner;

public class GuguTest {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("단을 입력하세요");
		int n = sc.nextInt();
		for (int i = n; i <= 9; i++) {
			System.out.printf("%d * %d = %d\n", n, i, n*i);
		}
	}
}
