package com.ssafy.movie;

public class Actor {
	
	String name;
	int age;
	String gender;
	String agency;
	
	public Actor() {
		super();
	}
	
	public Actor(String name, int age, String gender, String agency) {
		super();
		this.name = name;
		this.age = age;
		this.gender = gender;
		this.agency = agency;
	}
	
	void show() {
		System.out.println("이름 : "+ name);
		System.out.println("나이 : "+ age);
		System.out.println("성별 : "+ gender);
		System.out.println("소속 : "+ agency);		
	}
	
}
