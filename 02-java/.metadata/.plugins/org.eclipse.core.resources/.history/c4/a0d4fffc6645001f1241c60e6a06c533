package com.ssafy.class_extends;

public class Exam {
	
	public static void main(String[] args) {
		
		Person p = new Person();
		p.age = 10;
		p.name = "김길동";
		
		
		Student s = new Student();
		s.age = 20;
		s.name = "홍길동";
		s.major = "컴공";
		
		System.out.println(p.age);
		System.out.println(s.age);
	}
	
}
